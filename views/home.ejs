<%- include('partial/_nav') %>

<div class="btn-group" role="group" aria-label="Basic outlined example">
  <a href="/?priceNow=ASC">
    <button type="button" class="btn btn-outline-primary">Low Price</button>
  </a>
  <a href="/?priceNow=DESC">
    <button type="button" class="btn btn-outline-primary">High Price</button>
  </a>
</div>
<% data.forEach(el => { %>
<div class="card" style="width: 18rem;">
    <img src="<%= el.imageUrl %>" class="card-img-top" alt="...">
    <div class="card-body">
      <h5 class="card-title"><%= el.name %></h5>
      <p class="card-text"><%= el.description %></p>
      <a href="/details/<%= el.id %>" class="btn btn-outline-info">Rent Now</a>
    </div>
  </div>
  <% }) %>
<%- include('partial/_link') %>